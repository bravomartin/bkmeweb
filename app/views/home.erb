<%= erb :_header %>
<article id="home">
	<div id="map" class="home" zoom="12" url="<%=url_for "/", :full%>"></div>
	
	
	
	<section id="gets">
		<ul>
		<%@gets.each do |get|%>
			<li>
				<a class="get" href="/get/<%=get["tweet_id"]%>">
					<!-- <img class="map lazy" data-original="http://maps.googleapis.com/maps/api/staticmap?zoom=18&size=220x160&markers=icon:http://bkme.org/images/marker-06.png%7C<%=get["geolocation"]%>&sensor=true" src="/images/grey.png">
								<noscript> </noscript> -->
						<img class="map" src="http://maps.googleapis.com/maps/api/staticmap?zoom=18&size=220x160&markers=icon:http://bkme.org/images/marker-06.png%7C<%=get["geolocation"]%>&sensor=true" />

					<!-- <img class="photo lazy" width="200" data-original="<%="http://img.bkme.org/gets/#{get["filename"]}"%>" src="/images/grey.png" />
						<noscript>  </noscript>-->
						<img class="photo" src="<%="http://img.bkme.org/gets/#{get["filename"]}"%>" />
				</a>
				<a class="user" href="/user/<%=get["user_name"]%>">
					<span class="name"><%=get["user_name"]%></span>
					<span class="get"><%=@users_hash[get["user_name"]]%></span>
				</a>
			</li>
		<%end%>
		</ul>
	</section>
</article>
<%= erb :_footer %>
